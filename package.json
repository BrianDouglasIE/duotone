{
    "name": "duotone-with-workbench",
    "displayName": "DuoTone",
    "description": "Fork of Atom's DuoTone theme for Visual Studio Code",
    "version": "1.0.0",
    "publisher": "briandgls92",
    "scripts": {
        "build": "node ./src/build.js"
    },
    "engines": {
        "vscode": "^1.87.0"
